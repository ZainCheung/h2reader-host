{
  "version": 2,
  "builds": [
    {
      "src": "server/build.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/api.php",
      "dest": "/api/index.js"
    }
  ]
} 